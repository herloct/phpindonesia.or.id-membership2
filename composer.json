{
    "name": "phpindonesia/membership-2",
    "description": "PHP Indonesia Membership Application Reloaded",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "cloudinary/cloudinary_php": "dev-master",
        "google/recaptcha": "~1.1",
        "projek-xyz/slim-plates": "~0.2",
        "phpmailer/phpmailer": "^5.2",
        "slim/slim": "~3.1",
        "slim/flash": "~0.1",
        "slim/pdo": "~1.9",
        "vlucas/valitron": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Membership\\": "app/src/"
        },
        "files": [
            "app/src/helpers.php"
        ]
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "fzaninotto/faker": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Membership\\": [
                "app/tests/integration/"
            ]
        }
    }
}
